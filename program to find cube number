#include<stdio.h>
 
int main()
{
  int number, cube;
 
  printf(" \n Please Enter any integer Value : ");
  scanf("%d", &number);
  
  cube = number * number * number;
  
  printf("\n Cube of a given number %d is  =  %d", number, cube);
 
  return 0;
}

output:
Please Enter any integer Value : 6
Cube of a given number 6 is  =  216
